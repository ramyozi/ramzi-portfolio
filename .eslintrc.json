{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "react-hooks/exhaustive-deps": "off",
    "padding-line-between-statements": [
      "error",
      { "blankLine": "always", "prev": ["const", "let", "var"], "next": "*" },
      {
        "blankLine": "any",
        "prev": ["const", "let", "var"],
        "next": ["const", "let", "var"]
      },
      { "blankLine": "always", "prev": "if", "next": "*" }
    ]
  }
}
